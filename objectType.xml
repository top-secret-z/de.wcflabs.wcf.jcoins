            <name>de.wcflabs.jcoins.statement.register</name>
            <definitionname>de.wcflabs.jcoins.statement.object</definitionname>
            <defaultAmount>100</defaultAmount>
            <category>de.wcflabs.jcoins.wcf</category>
            <classname>\wcf\system\jcoins\statement\DefaultJCoinsStatement</classname>
            <objectClassName>\wcf\data\user\User</objectClassName>
        </type>

        <!-- like to reaction, just hide old like / dislike -->
        <type>
            <name>de.wcflabs.jcoins.statement.reaction</name>
            <definitionname>de.wcflabs.jcoins.statement.object</definitionname>
            <defaultAmount>25</defaultAmount>
            <retractable>1</retractable>
            <category>de.wcflabs.jcoins.wcf</category>
            <classname>\wcf\system\jcoins\statement\DefaultJCoinsStatement</classname>
            <objectClassName>*</objectClassName>
        </type>
        <type>
            <name>de.wcflabs.jcoins.statement.like</name>
            <definitionname>de.wcflabs.jcoins.statement.object</definitionname>
