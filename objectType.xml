            <classname><![CDATA[wcf\system\payment\method\JCoinsPaymentMethod]]></classname>
        </type>
        <type>
            <name>de.wcflabs.jcoins.payment.type.purchasableJCoins</name>
            <definitionname>com.woltlab.wcf.payment.type</definitionname>
            <classname><![CDATA[wcf\system\payment\type\PurchasableJCoinsPaymentType]]></classname>
        </type>

