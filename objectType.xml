            <classname><![CDATA[wcf\system\bulk\processing\user\ResetJCoinsUserBulkProcessingAction]]></classname>
            <action>resetJCoins</action>
            <permissions>admin.jcoins.canMassProcess</permissions>
        </type>
        <type>
            <name>de.wcflabs.jcoins.transfer</name>
            <definitionname>com.woltlab.wcf.bulkProcessing.user.action</definitionname>
            <classname><![CDATA[wcf\system\bulk\processing\user\TransferJCoinsUserBulkProcessingAction]]></classname>
            <action>transferJCons</action>
            <permissions>admin.jcoins.canMassProcess</permissions>
        </type>

