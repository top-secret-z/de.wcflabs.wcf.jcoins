            <retractable>1</retractable>
            <category>de.wcflabs.jcoins.conversation</category>
            <classname>\wcf\system\jcoins\statement\DefaultJCoinsStatement</classname>
            <objectClassName>\wcf\data\conversation\Conversation</objectClassName>
        </type>
        <type>
            <name>de.wcflabs.jcoins.statement.conversation.answer</name>
            <definitionname>de.wcflabs.jcoins.statement.object</definitionname>
            <defaultAmount>5</defaultAmount>
            <retractable>1</retractable>
            <category>de.wcflabs.jcoins.conversation</category>
            <classname>\wcf\system\jcoins\statement\DefaultJCoinsStatement</classname>
            <objectClassName>\wcf\data\conversation\message\ConversationMessage</objectClassName>
        </type>

        <type>
            <name>de.wcflabs.jcoins.statement.comment.comment</name>
            <definitionname>de.wcflabs.jcoins.statement.object</definitionname>
            <defaultAmount>10</defaultAmount>
            <retractable>1</retractable>
            <category>de.wcflabs.jcoins.comment</category>
            <classname>\wcf\system\jcoins\statement\DefaultJCoinsStatement</classname>
            <objectClassName>\wcf\data\comment\Comment</objectClassName>
