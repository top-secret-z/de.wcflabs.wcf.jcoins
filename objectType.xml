            <definitionname>de.wcflabs.jcoins.statement.object</definitionname>
            <editable>0</editable>
            <category>de.wcflabs.jcoins.wcf</category>
            <classname>\wcf\system\jcoins\statement\DefaultJCoinsStatement</classname>
            <objectClassName>\wcf\data\user\User</objectClassName>
        </type>
        <type>
            <name>de.wcflabs.jcoins.statement.transfer.fee</name>
            <definitionname>de.wcflabs.jcoins.statement.object</definitionname>
            <defaultAmount>0</defaultAmount>
            <category>de.wcflabs.jcoins.wcf</category>
            <classname>\wcf\system\jcoins\statement\DefaultJCoinsStatement</classname>
            <objectClassName>\wcf\data\user\User</objectClassName>
        </type>
        <type>
            <name>de.wcflabs.jcoins.statement.payment</name>
            <definitionname>de.wcflabs.jcoins.statement.object</definitionname>
